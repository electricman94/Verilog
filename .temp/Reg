#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xa0da10 .scope module, "Reg_tb" "Reg_tb" 2 29;
 .timescale 0 0;
P_0xa0db08 .param/l "WIDTH" 2 30, +C4<01000>;
P_0xa0db30 .param/l "clock_period" 2 31, +C4<010100>;
v0xa542d0_0 .var "clk", 0 0;
v0xa543a0_0 .net "out", 7 0, v0xa54180_0; 1 drivers
v0xa54450_0 .var "reset", 0 0;
S_0xa0e690 .scope module, "dut" "Reg" 2 37, 2 7, S_0xa0da10;
 .timescale 0 0;
P_0xa0d868 .param/l "WIDTH" 2 8, +C4<01000>;
v0xa20460_0 .net "clk", 0 0, v0xa542d0_0; 1 drivers
v0xa540e0_0 .net "in", 7 0, C4<10101011>; 1 drivers
v0xa54180_0 .var "out", 7 0;
v0xa54220_0 .net "reset", 0 0, v0xa54450_0; 1 drivers
E_0xa0ed90 .event posedge, v0xa20460_0;
    .scope S_0xa0e690;
T_0 ;
    %wait E_0xa0ed90;
    %load/v 8, v0xa54220_0, 1;
    %jmp/0xz  T_0.0, 8;
    %set/v v0xa54180_0, 0, 8;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0xa540e0_0, 8;
    %set/v v0xa54180_0, 8, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0xa0da10;
T_1 ;
    %set/v v0xa542d0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0xa0da10;
T_2 ;
    %delay 20, 0;
    %load/v 8, v0xa542d0_0, 1;
    %inv 8, 1;
    %set/v v0xa542d0_0, 8, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0xa0da10;
T_3 ;
    %vpi_call 2 57 "$dumpfile", "Reg.vcd";
    %vpi_call 2 58 "$dumpvars", 1'sb0, S_0xa0e690;
    %end;
    .thread T_3;
    .scope S_0xa0da10;
T_4 ;
    %delay 10, 0;
    %set/v v0xa54450_0, 1, 1;
    %delay 80, 0;
    %set/v v0xa54450_0, 0, 1;
    %delay 80, 0;
    %vpi_call 2 73 "$finish";
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "Reg.sv";
